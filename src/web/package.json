    "metascraper-url": "^5.24.8",
    "micro": "^9.3.4",
    "micro-cors": "^0.1.1",
    "nanoid": "^3.1.30",
    "next": "^12.0.4",
    "next-react-svg": "^1.1.2",
    "prismjs": "^1.24.1",
    "react": "^17.0.1",
    "react-dom": "^17.0.1",
    "react-firebase-hooks": "^4.0.1",
    "react-firebaseui": "^6.0.0",
    "react-hook-form": "^7.6.2",
    "react-modal": "^3.14.3",
    "react-select": "^5.2.1",
    "react-use-measure": "^2.0.4",
