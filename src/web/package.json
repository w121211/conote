{
  "name": "@conote/backend-nextjs",
  "version": "0.1.0",
  "license": "UNLICENSED",
  "private": true,
  "scripts": {
    "codegen": "graphql-let",
    "dev": "yarn codegen && NODE_OPTIONS=--max_old_space_size=4096 next dev",
    "build": "yarn codegen && next build",
    "start": "next start",
    "start:okteto": "next start -p 8080",
    "type-check": "tsc --pretty --noEmit",
    "format": "prettier --write .",
    "lint": "eslint . --ext ts --ext tsx --ext js",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "test-fetcher": "jest -c=jest.fetcher.config.js --watch",
    "test-prisma": "jest -c=jest.prisma.config.js --watch",
    "dbpush": "dotenv -e .env.local -- yarn run prisma db push",
    "migrate": "dotenv -e .env.local -- yarn run prisma migrate dev",
    "seed": "dotenv -e .env.local -- ts-node prisma/scripts/seed.ts ./prisma/seeds",
    "clean-types": "rm -rf dist && rm -rf reports",
    "build-types": "yarn run clean-types && yarn graphql-let && yarn tsc --build --verbose tsconfig.build-types.json",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook"
  },
  "husky": {
    "hooks": {
      "pre-commit": "lint-staged",
      "pre-push": "yarn run type-check"
    }
  },
  "lint-staged": {
    "*.@(ts|tsx)": [
      "yarn lint",
      "yarn format"
    ]
  },
  "dependencies": {
    "@apollo/client": "^3.3.15",
    "@auth0/nextjs-auth0": "^1.3.1",
    "@hookform/error-message": "^2.0.0",
    "@metascraper/helpers": "^5.24.8",
    "@prisma/client": "^3.4.1",
    "@svgr/webpack": "^6.0.0",
    "apollo-server-micro": "^3.5.0",
    "apollo3-cache-persist": "^0.13.0",
    "cookie": "^0.4.1",
    "cuid": "^2.1.8",
    "dotenv": "^10.0.0",
    "firebase": "^9.6.0",
    "firebase-admin": "^10.0.0",
    "fuse.js": "^6.4.6",
    "googleapis": "^92.0.0",
    "got": "^11.8.2",
    "graphql": "^15.0.0",
    "graphql-scalars": "^1.9.0",
    "jsonwebtoken": "^8.5.1",
    "localforage": "^1.10.0",
    "lodash": "^4.17.21",
    "metascraper": "^5.24.8",
    "metascraper-author": "^5.24.8",
    "metascraper-date": "^5.24.8",
    "metascraper-description": "^5.24.8",
    "metascraper-lang": "^5.24.8",
