  "scripts": {
    "build": "yarn prisma generate && yarn codegen && next build",
    "build-storybook": "build-storybook",
    "build-types": "yarn run clean-types && yarn graphql-let && yarn tsc --build --verbose tsconfig.build-types.json",
    "clean-types": "rm -rf dist && rm -rf reports",
    "codegen": "graphql-let",
    "dbpush": "dotenv -e .env.local -- yarn run prisma db push",
    "dev": "yarn codegen && NODE_OPTIONS=--max_old_space_size=4096 next dev",
    "format": "prettier --write .",
    "lint": "next lint",
    "migrate": "dotenv -e .env.local -- yarn run prisma migrate dev",
    "seed": "dotenv -e .env.local -- ts-node prisma/scripts/seed.ts ./prisma/seeds",
    "start": "next start",
    "start:okteto": "next start -p 8080",
    "storybook": "start-storybook -p 6006",
    "test": "jest",
    "test-all": "yarn lint && yarn type-check && yarn test",
    "test-fetcher": "jest -c=jest.fetcher.config.js --watch",
    "test-lib": "dotenv -e .env.local -- jest -c=./test/jest.lib.config.js --watch",
    "type-check": "tsc --pretty --noEmit"
  }
